/* pages/component/riderapplication/riderapplication.wxss */
.bdphone-wrap{
  height: 100vh;
  width: 100vw;
  background: #f6f4f3;
}
.bdphone{
  width: 100%rpx;
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
}
.bdphone input{
  height: 100rpx;
  width: 686rpx;
  margin-left: 32rpx;
  line-height: 100rpx;
  font-size: 32rpx;
  color: #999;
  border-bottom: 2rpx solid #ebebeb;
  box-sizing: border-box;
}
.bdyzm{
  width: 686rpx;
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 32rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  background: #fff;
}
.bdyzm-l{
  font-size: 32rpx;
}
.bdyzm-r{
  width: 192rpx;
}
.bdyzm-r button{
  width: 160rpx;
  font-size: 32rpx;
  background-color: #fff;
  padding: 0;
}
button::after{
  border: none;
  outline: 0;
}
.complete{
  margin-left: 76rpx;
  margin-top: 40rpx;
   width: 600rpx;
   height: 88rpx;
   line-height: 88rpx;
   text-align:center;
   background: #FF9021;
   border-radius: 50rpx;
   color: #fff;
   font-size: 32rpx;
}